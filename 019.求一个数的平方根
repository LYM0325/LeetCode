int mySqrt(int x){
   long int i = 0;
   while(1)
   {
       if(i*i<=x && (i+1)*(i+1)>x)
       {
        return i;
       }
       else
       {
           i++;
       }
   }
    return 0;
}
/////////////////////////////////////////////////////
int mySqrt(int x){
    long int a;
    for(a= 1;a*a<=x;a++)
    {
        continue;
    }
    return a-1;
}
